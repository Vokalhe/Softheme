#import <Foundation/Foundation.h>

int main(int argc, const char * argv[]) {
    @autoreleasepool {
        NSArray * artists = @[@"Vladimir Vysotsky", @"Rammstein", @"Wardruna", @"Foals ", @"Lemaitre feat. Jennie A"];
        NSArray * songs = @[@"Ballad of the fight", @"America", @"Heimta Thurs", @"Late Night", @"Closer"];
        for(int i = 0; i < artists.count; i++){
            NSLog(@"The song %@ by %@.", [songs objectAtIndex:i], [artists objectAtIndex:i]);
                
            }
    }
    return 0;
}
